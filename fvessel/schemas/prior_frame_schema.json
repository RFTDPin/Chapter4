{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "FVesselFramePrior",
  "type": "object",
  "required": [
    "frame_id",
    "timestamp",
    "mmsi",
    "u",
    "v",
    "sigma_r",
    "speed",
    "course",
    "confidence"
  ],
  "properties": {
    "frame_id": {"type": "integer"},
    "timestamp": {"type": "number"},
    "mmsi": {"type": "integer"},
    "u": {"type": "number"},
    "v": {"type": "number"},
    "sigma_r": {"type": "number", "minimum": 0},
    "speed": {"type": "number"},
    "course": {"type": "number"},
    "confidence": {"type": "number", "minimum": 0, "maximum": 1},
    "source": {"type": "string"},
    "notes": {"type": "string"}
  }
}
